default:
    pingboardConfig:
        clientID: '<CLIENT_ID>'
        clientSecret: '<CLIENT_SECRET>'
        excluded_status_types: []

    slackBotConfig:
        token: '<BOT_TOKEN>'

    slackSlashConfig:
        token: '<SLASH_TOKEN>'
        command: '/pingboard'
        port: 3000

    messageDelay: 60000
    userCacheRefreshInterval: 3600000
